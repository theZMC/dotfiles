[user]
	signingkey = zach@zmc.dev
	email = zach@zmc.dev
	name = Zach Callahan

[branch]
	autosetuprebase = always

[commit]
	gpgsign = true

[pull]
	autostash = true

[rerere]
	enabled = true

[include]
	path = ~/.gitconfig.local

[init]
	defaultBranch = main

[rebase]
	autoStash = true

[diff]
	compactionheuristic = true
	mnemonicprefix = true 

[fetch]
	prune = true
